function showZapin() {
    var zapinSession = sessionStorage.getItem('zapin');
    if (zapinSession == null) {
        document.getElementById('zapin').style.display = 'block';
        setTimeout(function() {
            document.getElementById('zapin').style.display = 'none';
        }, 5000);
        sessionStorage.setItem('zapin', true);
    }
}

function timeCloseZapin() {
    var startTime = new Date('2017/10/18 01:09:00'); // 设置砸屏开始时间
    var endTime = new Date('2017/10/18 10:18:00'); // 设置砸屏结束时间
    var nowTiem = new Date();
    if (startTime < nowTiem && endTime > nowTiem) {
        showZapin();
    }
}

timeCloseZapin();

document.querySelector('#zapin-close').addEventListener('click', function(ev) {
    closeZapin();
    ev.preventDefault();
    ev.stopPropagation();
});

function closeZapin() {
    document.getElementById('zapin').style.display = 'none';
}
