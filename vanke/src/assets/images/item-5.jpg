        function GetRTime(){
            var NowTime = new Date();
            var currentHour=NowTime.getHours(); //获取当前分钟数(0-23)
            var currentMin=NowTime.getMinutes();     //获取当前分钟数(0-59)
            var currentSed=NowTime.getSeconds();     //获取当前秒数(0-59)
            if(currentHour<10){
                var timeSession = "10点场",
                    countdown = "即将开始";
                setRtime(timeSession, countdown);
            }else if(currentHour>=10&&currentHour<11){
                var timeSession = "10点场",
                    countdown = "00"+ "时" + checkTime(60-(currentMin+1))+ "分" + checkTime(60-currentSed)+ "秒";
                setRtime(timeSession, countdown);
            }else if(currentHour>=11&&currentHour<13){
                var timeSession = "13点场",
                    countdown = "即将开始";
                setRtime(timeSession, countdown);
            }
            else if(currentHour>=13&&currentHour<14){
                var timeSession = "13点场",
                    countdown = "00"+ "时" + checkTime(60-(currentMin+1))+ "分" + checkTime(60-currentSed)+ "秒";
                setRtime(timeSession, countdown);
            }            
            else if(currentHour>=13&&currentHour<15){
                var timeSession = "15点场",
                    countdown = "即将开始";
                setRtime(timeSession, countdown);
            }else if(currentHour>=15&&currentHour<16){
                var timeSession = "15点场",
                    countdown = "00"+ "时" + checkTime(60-(currentMin+1))+ "分" + checkTime(60-currentSed)+ "秒";
                setRtime(timeSession, countdown);
            }
            else if(currentHour>=16&&currentHour<20){
                var timeSession = "20点场",
                    countdown = "即将开始";
                setRtime(timeSession, countdown);
            }else if(currentHour>=20&&currentHour<21){
                var timeSession = "20点场",
                    countdown = "00"+ "时" + checkTime(60-(currentMin+1))+ "分" + checkTime(60-currentSed)+ "秒";
                setRtime(timeSession, countdown);
            }
            else if(currentHour>=21&&currentHour<22){
                var timeSession = "22点场",
                    countdown = "即将开始";
                setRtime(timeSession, countdown);
            }else if(currentHour>=22&&currentHour<23){
                var timeSession = "22点场",
                    countdown = "00"+ "时" + checkTime(60-(currentMin+1))+ "分" + checkTime(60-currentSed)+ "秒";
